{
  "extends": [
    "config:base",
    ":gitSignOff",
    ":disableDependencyDashboard"
  ],
  "postUpgradeTasks": {
    "commands": [
      "make -C component gen-golden-all",
      "make -C package gen-golden-all"
    ],
    "fileFilters": [
      "component/tests/golden/**",
      "package/tests/golden/**"
    ],
    "executionMode": "update"
  },
  "suppressNotifications": [ "artifactErrors" ],
  "labels": [
    "dependency"
  ]
}
